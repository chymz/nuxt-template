{
  "name": "nuxt-template",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=nuxt* node server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write ./**/*.{js,vue}",
    "precommit": "npm run prettier && npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.1.0",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/onesignal": "^2.0.0",
    "@nuxtjs/pwa": "^2.0.5",
    "@nuxtjs/router": "^1.0.1",
    "animate-sass": "^0.8.2",
    "cross-env": "^5.1.3",
    "hover.css": "^2.2.1",
    "koa": "^2.4.1",
    "nuxt": "^1.0.0",
    "nuxt-class-component": "^1.2.0",
    "popmotion": "^8.1.9",
    "spectre.css": "^0.5.0",
    "vue-class-component": "^6.1.2",
    "vue-property-decorator": "^6.0.0",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "dashify": "^1.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-vue": "^4.0.0",
    "imagemin-webpack-plugin": "^1.6.1",
    "jest": "^22.1.4",
    "jest-vue-preprocessor": "^1.3.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "ts-jest": "^22.0.2",
    "ts-loader": "^3.3.1",
    "typescript": "^2.6.2",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-jarvis": "^0.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".vue$": "<rootDir>/node_modules/jest-vue-preprocessor",
      ".ts$": "<rootDir>/node_modules/ts-jest/preprocessor"
    },
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "globals": {
      "__TS_CONFIG__": "tsconfig.json",
      "__TRANSFORM_HTML__": true
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "testRegex": "/test/.*\\.(ts|js)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,ts,vue}"
    ],
    "mapCoverage": true,
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
